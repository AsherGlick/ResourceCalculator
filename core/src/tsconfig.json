{
	"compilerOptions": {
		"outDir": "../../cache/calculatorjs/",
		"rootDir": ".",
		"sourceMap": true,
		"module": "es2015",
		"target": "es2015",
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"strict": true,
		"downlevelIteration": true,
		"noImplicitAny": true,
		"strictNullChecks": true
	},
	"include": ["*.ts"],
	"exclude": ["**/*.test.ts"]
}
